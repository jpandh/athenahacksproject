{
  "manifest_version": 2,
  "name": "My Cool Extension",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*",
        "https://twitter.com/*",
        "https://www.reddit.com/*",
        "https://www.instagram.com/*"

      ],
      "js": ["content.js"]
    }
  ]
}
